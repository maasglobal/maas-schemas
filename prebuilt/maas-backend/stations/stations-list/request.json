{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.maas.global/maas-backend/stations/stations-list/request",
  "description": "Request schema for stations list",
  "type": "object",
  "properties": {
    "identityId": {
      "type": "string",
      "pattern": "^[aepsu]{2}\\x2D[0-9A-Z_a-z]{4}\\x2D[0-9]:[0-9a-f]{8}(\\x2D[0-9a-f]{4}){3}\\x2D[0-9a-f]{12}$"
    },
    "payload": {
      "type": "object",
      "properties": {
        "agencyId": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "type": "string",
          "minLength": 2,
          "maxLength": 64
        },
        "lat": {
          "description": "No-numeric precision version of MaaS core latitude",
          "type": "number",
          "minimum": -90,
          "maximum": 90
        },
        "lon": {
          "description": "No-numeric precision version of MaaS core latitude",
          "type": "number",
          "minimum": -90,
          "maximum": 90
        },
        "radius": {
          "type": "integer",
          "minimum": 0
        },
        "limit": {
          "type": "integer",
          "minimum": 0
        }
      },
      "required": [
        "agencyId",
        "lat",
        "lon"
      ]
    },
    "headers": {
      "Accept": {
        "description": "SemVer versioning schemas for versioning our resources",
        "type": "string",
        "pattern": "\\bapplication\\/json[,;][\\t-\\r \\xA0\\u1680\\u2000-\\u200A\\u2028\\u2029\\u202F\\u205F\\u3000\\uFEFF]*version=((0|[1-9][0-9]*)\\.(0|[1-9][0-9]*)\\.(0|[1-9][0-9]*)(\\x2D[\\x2D0-9a-z]+(?:\\.[\\x2D0-9a-z]+)*)?(?:\\+[\\x2D0-9a-z]+(?:\\.[\\x2D0-9a-z]+)*)?)\\b"
      },
      "X-Whim-User-Agent": {
        "type": "string",
        "minLength": 1
      }
    }
  },
  "required": [
    "identityId",
    "payload"
  ],
  "additionalProperties": false
}