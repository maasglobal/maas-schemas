{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.maas.global/core/aws-units",
  "description": "AWS common units",
  "definitions": {
    "arn": {
      "type": "string",
      "pattern": "^arn:aws:[a-z]+:[\\-0-9A-Z_a-z]*:[0-9]{12}:[ \\-0-9A-Z_a-z]+(\\/[ \\-\\/-9A-Z_a-z]+)*",
      "minLength": 1,
      "maxLength": 256
    },
    "identityId": {
      "type": "string",
      "pattern": "^[aepsu]{2}\\-[0-9A-Z_a-z]{4}\\-[0-9]:[0-9a-f]{8}(\\-[0-9a-f]{4}){3}\\-[0-9a-f]{12}$"
    },
    "deviceToken": {
      "type": "string",
      "pattern": "^([0-9A-Fa-f]{2}){8,64}$"
    }
  }
}