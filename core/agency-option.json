{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.maas.global/v1/units",
  "description": "MaaS Single agency option response",
  "type": "object",
  "required": [
    "leg",
    "meta",
    "terms"
  ],
  "properties": {
    "leg": {
      "type": "object",
      "properties": {
        "startTime": {
          "$ref": "units.json#definitions/time"
        },
        "endTime": {
          "$ref": "units.json#definitions/time"
        },
        "from": {
          "$ref": "units.json#definitions/location"
        },
        "to": {
          "$ref": "units.json#definitions/location"
        },
        "agencyId": {
          "type": "string"
        }
      }
    },
    "meta": {
      "type": "object",
      "description": "Details and specs of the vehicle"
    },
    "terms": {
      "type": "object",
      "description": "Tsp terms of usage / order"
    },
    "signature": {
      "type": "string",
      "description": "This object's signature"
    }
  },
  "definitions": {
    "identityId": {
      "$ref": "aws.json#/definitions/cognitoIdentityId"
    }
  }
}
