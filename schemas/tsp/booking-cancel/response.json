      "$ref": "../../core/booking.json#/definitions/meta"
    },
    "terms": {
      "$ref": "../../core/booking.json#/definitions/terms"
    },
    "token": {
      "$ref": "../../core/booking.json#/definitions/token"
    },
    "tspProduct": {
      "$ref": "../booking-option.json#/definitions/tspProduct"
    }
  },
